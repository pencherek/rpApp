    <InfoLines>
     <InfoSQL caption="Dernières modifications">select userid, concat(left(timestamp,8),' ',substring(timestamp,9,20)) as timestamp, 
      case libaction 
          when 'ExecuterSuivi' then '&lt;i class=" fa fa-fw fa-gavel"&gt;&lt;/i&gt;' 
          when 'BeforeModState' then '&lt;i class=" fa fa-fw fa-chevron-circle-right"&gt;&lt;/i&gt;' 
          when 'AfterModState' then '&lt;i class=" fa fa-fw fa-check-circle"&gt;&lt;/i&gt;' 
          else '' end as mark,
      libaction, description 
      FROM  LogTblAudit where R0 = '{#0#}' and libaction not in ('AfterOpenDoc')
      order by timestamp desc, description desc  limit 300</InfoSQL>
    </InfoLines>
