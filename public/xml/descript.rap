    <InfoLines>
     <InfoSQL caption="Historique des séances" position="bottom">SELECT distinct concat('javascript:RAP.loaddirect({doc:''', DtaTbl00.R0, '''})') as "Ouvrir@fa fa-fw fa-folder-open", R1 as "Description", iso2txt(R040120) as "Date de la séance", 
     AutoTblStatut.Libelle as "Etat de la séance", 
     case R046001 when 1 then coalesce(d.Libelle,'-') else '-' end as "Décision", 
     case when idStatut in ('Planification','Agendee','FinalisationOJ') and '{$data.getCurrentGroup$}' like '%ACTEURL%' then concat('javascript: RAP.evaluate({files:''affaireAction'',params:''RetraitSeance;',DtaTbl00.R0,'''})') end as "Retirer de l'OJ@fa fa-fw fa-close"
from DtaTbl00 inner join DtaTbl046m on DtaTbl00.R0 = DtaTbl046m.r0
inner join DtaTbl040s on DtaTbl00.R0 = DtaTbl040s.r0
left join CstTblDecisionAffaire d on R046180 = d.IdCst and Entite = R040140
inner join AutoTblStatut on R999999 = IdStatut and IdModele = R999997
where R046105 = '{#0#}'
order by R040120 desc</InfoSQL>
