    <InfoLines>
     <InfoSQL caption="_TBLMESSAGE">SELECT MsgType,Message from TmpTblDocMessage WHERE RapId = '{#0#}' AND UPPER(userId) = UPPER('{$data.getCurrentUser$}') ORDER BY MsgType,Ordre</InfoSQL>
     <InfoSQL caption="Historique">SELECT case when version like '%**' then 'please_hide_me'
                                                else concat('javascript:doOpenDocument(''', nodeid, ';', version, '''), 0, ''getHistory'')') end as "Ouvrir@fa fa-fw fa-folder-open",
                                            title as "Titre",
                                            version as "Version",
                                            modby as "Modifié par",
                                            iso2txt(moddatetime) as "Modifié le/à",
                                            case when version like '%**' then 'please_hide_me'
                                                else concat('javascript:RAP.evaluate({files:''delHistory'',params:''', title, ';', nodeid, ';', version, '''})') end as "Supprimer@fa fa-fw fa-trash"
                                          FROM
                                            DtaTblDocHistory where R0 = '{#0#}'  and UPPER(userId) = UPPER('{$data.getCurrentUser$}')
                                          ORDER BY moddatetime DESC
      </InfoSQL>
     <InfoSQL caption="_BUTTON" target="Historique">SELECT 'javascript:RAP.evaluate({files:''showHistory'',params:''''})' as "onclick",
                                                            'btn btn-primary btn-inverse' as "class",
                                                            '&lt;span class=''fa fa-fw fa-times''&gt;&lt;/span&gt; Fermer l''historique' as "label"
                                                          FROM    DtaTblDocHistory where R0 = '{#0#}'  and UPPER(userId) = UPPER('{$data.getCurrentUser$}') limit 1
      </InfoSQL>
     <InfoSQL hide="1" caption="infosDocument">SELECT distinct R916218 data_key,COALESCE(n.libelle,'') data_r060260,COALESCE(e.libelle,'') data_r060265, iso2txt(R060270) data_r060270, iso2txt(R060205) data_r060205, case when coalesce(R060291,'')='' then '' else concat(R060291, ', ', iso2txt(R060292)) end data_r060291 FROM DtaTbl060s INNER JOIN DtaTbl916m ON R916218=DtaTbl060s.R0 LEFT JOIN CstTblNatureDocument n ON n.idCst=R060260 LEFT JOIN CstTblStatutDocument e ON e.idCst=R060265 WHERE DtaTbl916m.R0='{#0#}'</InfoSQL>
